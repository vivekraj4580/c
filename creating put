class demo 
{
    int a,b;
    public void get (int x, int y){
        a =x;
        b = y;
    }
    public void put()
    {
       System.out.println("a = "+a);
       System.out.println("b = "+b);
    }
   

public static void main(String[] args){
    demo obj = new demo();
    obj.get(10, 40);
    obj.put();
}
}